#version 330 core

layout (location = 0) out vec4 color1;
layout (location = 1) out vec4 color2;

in vec4 myvertex;
in vec3 mynormal;
in vec2 mytexturecoordinate;

uniform sampler2D texAlbedo;
uniform sampler2D texAO;
uniform sampler2D texHeight;
uniform sampler2D texMetal;
uniform sampler2D texNormal;
uniform sampler2D textRough;

void main (void)
{   
	color1 = texture(texAO, mytexturecoordinate.st);
	color2 = texture(texHeight, mytexturecoordinate.st);
}